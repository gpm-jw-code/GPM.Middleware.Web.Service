		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Log\**" />
		<Compile Remove="log_service_frontend\**" />
		<Compile Remove="ssm\**" />
		<Content Remove="Log\**" />
		<Content Remove="log_service_frontend\**" />
		<Content Remove="ssm\**" />
